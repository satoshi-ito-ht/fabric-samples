{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "trailing-comma": [
            true,
            { "multiline": "always", "singleline": "never" }
        ],
        "object-literal-sort-keys": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
